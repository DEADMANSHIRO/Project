#include <iostream>
#include <cmath>

using namespace std;

struct Point
{
    double x;
    double y;
};

double distance(const Point& p1, const Point& p2)
{
    return sqrt(pow(p2.x - p1.x, 2) + pow(p2.y - p1.y, 2));
}

double rectangleArea(const Point& p1, const Point& p2, const Point& p3, const Point& p4)
{
    double side1 = distance(p1, p2);
    double side2 = distance(p2, p3);
    return side1 * side2;
}

int main()
{
    setlocale(LC_ALL,"Rus");

    Point p1, p2, p3, p4;

    cout << "Введите координаты четырех вершин прямоугольника:" << endl;
    cout << "Первая вершина (x1 y1): ";
    cin >> p1.x >> p1.y;
    cout << "Вторая вершина (x2 y2): ";
    cin >> p2.x >> p2.y;
    cout << "Третья вершина (x3 y3): ";
    cin >> p3.x >> p3.y;
    cout << "Четвертая вершина (x4 y4): ";
    cin >> p4.x >> p4.y;

    double area = rectangleArea(p1, p2, p3, p4);
    cout << "Площадь прямоугольника: " << area << endl;

    return 0;
}
